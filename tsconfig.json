{
  "compilerOptions": {
    "target": "ES2016",
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "build",
    "jsx": "react",
    "jsxFactory": "h",
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "functions"]
}
